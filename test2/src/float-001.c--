target byteorder little;

bits8 c;

section "data" 
{
    x: bits64[] { 3.1415926535::bits64 };
}

export main; import printf;
foreign "C" main (bits32 argc, "address" bits32 argv) {
  bits64 y;
  y = bits64[x];
  foreign "C" printf("address" fmt, y);
  foreign "C" return (0);
}

section "data" {
  fmt: bits8[] "a hand-written literal pi is %lg\n\0";
}

